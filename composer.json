        "slim/twig-view": "^2.1",
        "slim/http-cache": "^0.3.0",
        "tuupola/slim-basic-auth": "^2.0",
        "slim/flash": "dev-master",
        "doctrine/orm": "^2.5",
        "symfony/dom-crawler": "^3.1",
